   "source": [
    "model = Sequential()\n",
    "\n",
    "#the input layer\n",
    "model.add(Flatten(input_shape=[28,28]))\n",
    "\n",
    "#now we add the hidden layer\n",
    "model.add(Dense(50,activation='relu'))\n",
    "model.add(Dropout(0.2))\n",
    "model.add(Dense(10,activation='softmax'))"
